{
  "name": "express-ts-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tspc",
    "build1": "tsc && tsc-alias",
    "start:product": "node dist/app.js",
    "worker:product": "node dist/worker.js",
    "start:all": "set NODE_ENV=production&&set PORT=3001&& concurrently \"npm:start:product\" \"npm:worker:product\"",
    "start": "set NODE_ENV=development&&set PORT=3001&&nodemon --ignore storage -r tsconfig-paths/register -r ts-node/register ./src/app.ts",
    "worker": "set NODE_ENV=development&&nodemon --ignore storage -r tsconfig-paths/register -r ts-node/register ./src/worker.ts",
    "start:dev": "set NODE_ENV=development&&nodemon --ignore storage -r tsconfig-paths/register ./src/app.ts",
    "start:prod": "set NODE_ENV=production&&node --ignore storage -r tsconfig-paths/register ./dist/app.js",
    "exec": "node ./dist/app.js"
  },
  "keywords": [],
  "type": "commonjs",
